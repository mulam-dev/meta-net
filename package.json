{
	"name": "template",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host",
		"deno": "deno run -A index.js",
		"build": "vite build && cp deno.json build/deno.json",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@deno/vite-plugin": "latest",
		"@iconify-json/material-symbols": "^1.2.6",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@unocss/core": "latest",
		"@unocss/extractor-svelte": "latest",
		"@unocss/preset-icons": "latest",
		"@unocss/preset-uno": "latest",
		"@unocss/reset": "latest",
		"@unocss/svelte-scoped": "latest",
		"@unocss/transformer-directives": "latest",
		"@unocss/transformer-variant-group": "latest",
		"runed": "^0.15.3",
		"svelte": "^5.0.0",
		"svelte-adapter-deno-native": "^0.9.2",
		"svelte-check": "^4.0.0",
		"sveltekit-sse": "^0.13.9",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"unocss": "latest",
		"unocss-preset-radix": "latest",
		"vite": "^5.4.4",
		"vitest": "^2.1.4"
	},
	"type": "module",
	"dependencies": {
		"msgpackr": "^1.11.2"
	}
}
